{
  "expo": {
    "name": "simple todo app",
    "slug": "todo",
    "version": "2.0.1",
    "description": "A quick and simple todo list for maintaining important day to day activities",
    "orientation": "portrait",
    "githubUrl": "https://github.com/madan712/simple-todo-app",
    "privacy": "public",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#3b5998"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.javaxp.todo",
      "buildNumber": "2.0.1"
    },
    "android": {
      "permissions": [
        "CAMERA",
        "RECORD_AUDIO",
        "READ_PHONE_STATE",
        "READ_CONTACTS"
      ],
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.javaxp.todo",
      "versionCode": 4,
      "playStoreUrl": "https://play.google.com/store/apps/details?id=com.javaxp.todo"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
